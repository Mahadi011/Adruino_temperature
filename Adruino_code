#include <DHT.h>
#define Type DHT11
int sensePin=2; //Have to select pin 2 for the signal
DHT HT(sensePin,Type);
float tempC;
int dt=1000;
int settime=500;

void setup()
{
Serial.begin(9600);
HT.begin();
delay(settime);
}
void loop()
{
  tempC = HT.readTemperature();
  humidity = HT.readHumidity;
  Serial.print(humidity);
  Serial.print(tempC);
  Serial.println("c");
  delay(dt);
}
